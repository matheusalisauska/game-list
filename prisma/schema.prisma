

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Games {
  id String @id @default (uuid())
  name String
  platform Platform @default(PC)
  imageUrl String
  finished Boolean
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}


enum Platform {
  PC
  PlayStation
  Mobile
}
